<h2><i class="fa fa-print"></i> <%= @edi_printer.name %></h2>
<hr/>

<%= simple_form_for [:admin, @edi_printer] do |f| %>
  
  <%= render 'admin/shared/error_messages', object: f.object %>
  <%= f.input :name %>
  <%= f.input :hostname %>
  <%= f.input :port %>
  <%= f.input :location %>
  <%= f.button :submit %>

<% end %>




<% content_for :title do %>
	Brands: <%= @edi_printer.name %>
<% end %>



<% content_for :breadcrumbs do %>
	<li><a href="#">EDI Settings</a></li>
	<li><%= link_to "Printers", admin_edi_printers_path %></li>
	<li class="active"><%= @edi_printer.name %></li>
<% end %>